{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "pdflatex",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/build"
			},
			"command": "pdflatex.exe -synctex=1 -interaction=nonstopmode --shell-escape \"template\".tex",
			"problemMatcher": [],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "biber",
			"type": "shell",
			"options": {
				"cwd": "${workspaceFolder}/build"
			},
			"command": "biber.exe \"template\"",
			"group": "build"
		}
	]
}
